Vr.B.05 = var(data$B.05)
Vr.B.06 = var(data$B.06)
Vr.B.07 = var(data$B.07)
Vr.B.08 = var(data$B.08)
Vr.B.09 = var(data$B.09)
Vr.B.10 = var(data$B.10)
Vr.B.11 = var(data$B.11)
Vr.B.12 = var(data$B.12)
